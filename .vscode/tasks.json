{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "npm: start:debug",
      "type": "shell",
      "command": "npm run start:debug",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "pattern": {
          "regexp": ".*"
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*"
        }
      }
    }
  ]
}
